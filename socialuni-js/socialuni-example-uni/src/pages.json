{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
    }
  },
  "pages": [
    {
      "path": "pages/user/mine",
      "style": {
        "navigationStyle": "custom"
      }
    }
  ],
  "subPackages": [
    {
      "root": "pagesLazy/user",
      "pages": [
        {
          "path": "userDetail",
          "style": {
            "navigationStyle": "custom"
          }
        },
        {
          "path": "editMineInfo",
          "style": {
            "navigationBarTitleText": "编辑资料"
          }
        },
        {
          "path": "userImgList",
          "style": {
            "navigationBarTitleText": "相册列表"
          }
        },
        {
          "path": "follow",
          "style": {
            "navigationBarTitleText": "关注列表"
          }
        },
        {
          "path": "coin",
          "style": {
            "navigationBarTitleText": "贝壳充值",
            "enablePullDownRefresh": true
          }
        },
        {
          "path": "coinRecord",
          "style": {
            "navigationBarTitleText": "贝壳记录"
          }
        }
      ]
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "社区",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8",
    "app-plus": {
      "softinputNavBar": "none"
    },
    "h5": {
      "maxWidth": 800
    }
  },
  "navigateToMiniProgramAppIdList": [
    "1109985787"
  ]
}

